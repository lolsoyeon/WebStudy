<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Test009.html</title>

<script type="text/javascript">

	document.write("<div><h1>관계 연산자와 논리 연산자</h1><hr></div>");
	
	num1 = 7;
	num2 = 3;
	
	res = (num1 == num2);
	document.write("num1 == num2 → ");
	document.write(res);
	
	res = (num1 >= num2);
	document.write("<br>num1 >= num2 → ");
	document.write(res);
	
	res = (num1 <= num2);
	document.write("<br>num1 <= num2 → ");
	document.write(res);
	
	document.write("<br><br><br><br><br>");
	
	res = (num1 < 10 && num2 <10);
	document.write("<br>num1 < 10 && num2 < 10 → ");
	document.write(res);
	
	res = (num1 > 10 && num2 > 10);
	document.write("<br>num1 > 10 && num2 < 10 → ");
	document.write(res);
	
	res = (num1 > 10 || num2 > 10);
	document.write("<br>num1 > 10 || num2 > 10 → ");
	document.write(res);
	
	res = (num1 > 10 || num2 < 10);
	document.write("<br>num1 > 10 || num2 < 10 → ");
	document.write(res);
	
	res = !(num1 == num2);
	document.write("<br>!(num1 == num2) → ");
	document.write(res);
	
	document.write("<br><br><br><br>");
	

</script>

</head>
<body>

</body>
</html>